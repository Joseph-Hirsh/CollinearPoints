import java.util.ArrayList;
import java.util.Arrays;

public class Brute {
    private LineSegment[] lineSegments;

    //find all line segments containing 4 points
    public Brute(Point[] points) {
        ArrayList<LineSegment> storeSegments = new ArrayList<LineSegment>();
        Arrays.sort(points);
        int length = points.length;
        for (int i = 0; i < length; i++) {
            Point one = points[i];
            for (int j = 1; j < length; j++) {
                Point two = points[j];
                for (int k = 2; k < length; k++) {
                    Point three = points[k];
                    for (int l = 3; l < length; l++) {
                        Point four = points[l];
                        if (one.slopeTo(two) == one.slopeTo(three) && one.slopeTo(two) == one.slopeTo(four)) {
                            storeSegments.add(new LineSegment(one, four));
                        }
                    }
                }
            }
        }
        lineSegments = storeSegments.toArray(new LineSegment[0]);
    }

    // the number of line segments
    public int numberOfSegments() {
        return lineSegments.length;
    }

    // the line segments
    public LineSegment[] segments() {
        return lineSegments.clone();
    }
}
